<% if @usuario.errors.count == 0 %>
	$('#formUsuario').remove();
	$('#usuarios').show();
	$('#new_usuario_link').show();
	$('#listar_usuarios_link').hide();
	$('#usuarios').append('<%= j render("usuario", :usuario => @usuario) %>');
	document.getElementById("tituloUsuario").innerHTML = "Usuarios";
	document.getElementById("noticeMsg").innerHTML = "Se creo correctamente el usuario";
	$("#noticeMsg").toggle( "fade" );
	window.location = 'procesos'
<% else %>
	$('#formUsuario').remove();
	$('#headersingup').remove();
	$('#formularioUsuarios').after('<%= j render("formNuevoDespacho") %>');
	$("#modalsignup").foundation("reveal", "open")  
<% end %>

