<!DOCTYPE html>
<html>
<head>
  <title>AspenVersion3</title>

<%= include_gon  %>

<%= stylesheet_link_tag    "application", :media => "all" %>
<%= javascript_include_tag "application" %>
<%= javascript_include_tag "autocomplete-rails.js" %>
<%= csrf_meta_tags %>

<script src="/assets/jquery-ui-timepicker-addon.js?body=1" type="text/javascript"></script>
<script src="/assets/fullcalendar/fullcalendar.js?body=1" type="text/javascript"></script>
<script src="/assets/fullcalendar/fullcalendar.min.js?body=1" type="text/javascript"></script>
<script src="/assets/fullcalendar/gcal.js?body=1" type="text/javascript"></script>
<link href="/assets/fullcalendar/fullcalendar.css?body=1" media="all" rel="stylesheet" type="text/css">

</head>
<body>

<% if current_user %>
	<%= render "layouts/header" %>
<% else %>
	<%= render "layouts/headerLogin" %>
<% end %>

<%= yield %>

</body>

<% if current_user %>
	<%= render "layouts/footer" %>
<% else %>
	<%= render "layouts/footerLogin" %>
<% end %>


<%= render "layouts/scriptFoundation" %>

</html>