<body>

  <div id="workspace" class="workspace">
    <div class="container stack_container" style="width: 960px;">
      <div class="panel sheet project active blank_slate fondoGris">

        
        <header>
              <ul class="breadcrumbs">
                <li><%= link_to @proceso, :class => "tituloProceso label round" do  %> 
                      <%= @proceso.tipo_proceso.tipo %> : <%= @proceso.titulo %>
                    <% end %>
                </li>
                 <li> <%= link_to   "Conclusiones", {:controller => "estados", :action => "index", :proceso_id => params[:proceso_id]}, :method=>:get %></li>
            <li><a href="#">Nueva Conclusion</a></li>
              </ul>
            </header>

        <div class="panel sheet topics fondoGris"
            style="margin-left: 20px; margin-bottom: -20px;"
            data-behavior=" prevent_reload_when_stacked">

          <header class="has_buttons">
            <div data-behavior="edit_project_header expandable">
              <div class="collapsed_content ">
                <div class="row">
                  <div class="small-12 columns">
                    <div class="small-6 large-9 columns left">
                         <h1 id="tituloContrato">
                           Nueva Conclusion
                         </h1>
                         </div>
                        <div class="small-4 large-3 columns left">
                        <%= link_to   "Listar Conclusiones", {:controller => "estados", :action => "index", :proceso_id => params[:proceso_id]}, :method=>:get, :class => "button radius "%>
                  </div>
                  </div>
                </div>
              </div>
            </div>
          </header>
          <div class="sheet_body ">
            <article class="calendar_event new expanded" >
              <div class="expanded_content ">
                <%= render 'form' %>
              </div>
            </article>
          </div>
        </div>
      </div>
    </div>
  </div>
</body>