<h1>Listing tipo_procesos</h1>

<table>
  <tr>
    <th>Tipo</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tipo_procesos.each do |tipo_proceso| %>
  <tr>
    <td><%= tipo_proceso.tipo %></td>
    <td><%= link_to 'Show', tipo_proceso %></td>
    <td><%= link_to 'Edit', edit_tipo_proceso_path(tipo_proceso) %></td>
    <td><%= link_to 'Destroy', tipo_proceso, method: :delete, data: { confirm: 'Are you sure?' } %></td>
